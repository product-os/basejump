{
  "name": "basejump",
  "version": "0.0.1",
  "private": true,
  "description": "A GitHub app for rebasing on demand",
  "author": "Balena",
  "license": "Apache-2.0",
  "homepage": "https://github.com/balena-io-experimental/basejump",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc --project ./tsconfig.release.json",
    "lint": "balena-lint -e ts src test",
    "lint:fix": "balena-lint --fix -e ts src test",
    "start": "probot run ./lib/index.js",
    "test": "npm run build && vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "probot": "^13.4.3"
  },
  "devDependencies": {
    "@balena/lint": "^9.1.3",
    "@types/node": "^20.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nock": "^14.0.1",
    "rimraf": "^6.0.1",
    "smee-client": "^3.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "type": "module"
}
